<form action="/dogs" method="GET">
    <label for="nameFilter">Filter by Name</label>
    <input type="text" id="nameFilter" name='nameFilter'>
    <input type="submit">
    </form>
    
    <ul>
        <% myDogs.forEach((dog, index) => { %>
            <div class="tooltip">
            <li><%= dog.name %> is famous for <%= dog.famousFor %> and looks like this: <br><img id="dogDisplay" src="<%= dog.image %>"/>
            </div>
                <a id='editLink' href="/dogs/edit/<%= index %>">Edit</a>
                <form id="deleteForm" action="/dogs/<%= index %>/?_method=DELETE" method="POST">
                <input  type="submit" value="Delete">
            </form>
            </li>
        
        <% }) %>
    </ul>