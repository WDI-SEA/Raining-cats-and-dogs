<form method="POST" action="/dogs/<%= dogId %>/?_method=PUT">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" value="<%= dog.name %>" required>
    <label for="image">Image URL: </label>
    <input type="text" id="image" name="image" value="<%= dog.image %>" required>
    <label for="dog-fame">Famous for...</label>
    <input type="text" id="dog-fame" name="famousFor" value="<%= dog.famousFor %>" required>
    <input type="submit">
</form>