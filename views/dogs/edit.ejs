<form method="POST" action="/dogs/<%= dogId %>/?_method=PUT">
    <label for="dogName">Name</label>
    <input id="dogName" type="text" name="name" value="<%= dog.name %>">
    <label for="dogType">type</label>
    <input id="dogType" type="text" name="type" value="<%= dog.type %>">
    <label for="dogFamousFor">FamousFor</label>
    <input id="dogFamousFor" type="text" name="famousFor" value="<%= dog.famousFor %>">
    <input type="submit">
</form>