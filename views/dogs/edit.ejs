<form method="POST" action="/dogs/<%= dogId %>/?_method=PUT">
    <label for="dogName">Name</label>
    <input id="dogName" type="text" name="name" value="<%= dog.name %>">
    <label for="famousFor">Famous For:</label>
    <input id="famousFor" type="text" name="famousFor" value="<%= dog.famousFor %>">
    <label for="image">Link a picture here:</label>
    <input id="image" type="text" name="image" value="<%= dog.image %>">
    <input type="submit">
</form>