<form method="POST" action="/cats/<%= catsId %>/?_method=PUT">
    <div>
    <label for="catsName">Name</label>
    <input id="catsName" type="text" name="name" value="<%= cats.name %>">
    </div>
    <div>
    <label for="catsImage">Image</label>
    <input id="catsImage" type="text" name="type" value="<%= cats.image %>">
    </div>
    <div>
    <label for="catsFamous">Famous For</label>
    <input id="catsFamous" type="text" name="type" value="<%= cats.famousFor %>">
    </div>
    <div>
    <input type="submit">
    </div>
</form>