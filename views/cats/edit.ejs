<form action="/cats/<%- catID %>/?_method=PUT" method="POST">
    <label>Name</label>
    <input type="text" name='name' value="<%= cat.name %>">
    <label>Photo</label>
    <input type="url" name="type" value="<%= cat.image %>">
    <label>Famous for?</label>
    <input type="text" name="type" value="<%= cat.famousFor %>">
    <input type="submit">
</form>