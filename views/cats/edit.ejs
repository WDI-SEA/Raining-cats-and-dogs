<form method="POST" action="/cats/<%= catId %>/?_method=PUT">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" value="<%= cat.name %>">
    <label for="image">Image URL: </label>
    <input type="text" id="image" name="image" value="<%= cat.image %>">
    <label for="cat-fame">Famous for...</label>
    <input type="text" id="cat-fame" name="famousFor" value="<%= cat.famousFor %>">
    <input type="submit">
</form>